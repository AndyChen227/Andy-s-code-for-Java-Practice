import java.util.*;
public class WriteSequence {
    public static void writeSequence(int n) {
        // TODO: Your code here
        // Split to two parts
        if (n < 1) {
            throw new IllegalArgumentException("n can not smaller" + 
            "than 0");
        } 
        if (n == 1) {
            System.out.print(1);
        } else if (n == 2) {
            System.out.print("1 1");
        } else {
            int number = (n + 1) / 2;
            System.out.print(number + " ");
            writeSequence(n - 2);
            System.out.print(" " + number);
        }
    }

    public static void main(String[] args) {
        writeSequence(8);
    }
}
